extends partials/layout


block content
	#todo-list.row
		ul.fly-reverse
			li(ng-class='pass ', ng-repeat="todo in todos | filter:#{model} | orderBy:'-_id' ")
				input(id='cbtest_{{todo._id}}', type='checkbox', ng-click='deleteTodo(todo._id)')
				label.check-box(for='cbtest_{{todo._id}}')
				span {{ todo.text }}


				
